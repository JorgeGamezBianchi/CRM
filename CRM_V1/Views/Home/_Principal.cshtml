<script>
    //manejador de collapse
    $(".glyphicon-chevron-up").each(function () {
        $(this).parent().find(".glyphicon").removeClass("glyphicon-chevron-up")
            .addClass("glyphicon-chevron-down");
    })

    $(".collapse").on("show.bs.collapse", function () {
        $(this).parent().find('.glyphicon-chevron-down').removeClass("glyphicon-chevron-down")
            .addClass("glyphicon-chevron-up");
    }).on("hide.bs.collapse", function () {
        $(this).parent().find('.glyphicon-chevron-up').removeClass("glyphicon-chevron-up")
            .addClass("glyphicon-chevron-down");
    });
</script>
